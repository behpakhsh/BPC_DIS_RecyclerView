<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DisRecyclerView">

        <attr name="disGoUpEnable" format="boolean" />
        <attr name="disGoUpSrc" format="reference|color" />

        <attr name="disTableOrientation">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
            <enum name="grid" value="2" />
        </attr>

        <attr name="disScrollbars">
            <flag name="none" value="0x00000000" />
            <flag name="horizontal" value="0x00000100" />
            <flag name="vertical" value="0x00000200" />
            <flag name="horizontalAndVertical" value="0x00000300" />
        </attr>

        <attr name="disOverScrollMode">
            <enum name="always" value="0" />
            <enum name="ifContentScrolls" value="1" />
            <enum name="never" value="2" />
        </attr>

        <attr name="disLayoutDirection">
            <enum name="ltr" value="0" />
            <enum name="rtl" value="1" />
            <enum name="inherit" value="2" />
            <enum name="locale" value="3" />
        </attr>

        <attr name="disNumberOfColumns" format="integer" />
        <attr name="disBottomSpace" format="dimension" />

        <attr name="disDividerEnable" format="boolean" />
        <attr name="disDividerSrc" format="reference|color" />

    </declare-styleable>
</resources>