<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DisRecyclerView">

        <attr name="goUpEnable" format="boolean" />
        <attr name="goUpSrc" format="reference|color" />

        <attr name="tableOrientation">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
            <enum name="grid" value="2" />
        </attr>

        <attr name="goUpHeight" format="dimension">
            <enum name="fill_parent" value="-1" />
            <enum name="match_parent" value="-1" />
            <enum name="wrap_content" value="-2" />
        </attr>

        <attr name="goUpWidth" format="dimension">
            <enum name="fill_parent" value="-1" />
            <enum name="match_parent" value="-1" />
            <enum name="wrap_content" value="-2" />
        </attr>

        <attr name="numberOfColumns" format="integer" />

        <attr name="dividerEnable" format="boolean" />
        <attr name="dividerSrc" format="reference|color" />

        <attr name="goUpMargin" format="dimension" />
        <attr name="goUpMarginTop" format="dimension" />
        <attr name="goUpMarginBottom" format="dimension" />
        <attr name="goUpMarginStart" format="dimension" />
        <attr name="goUpMarginEnd" format="dimension" />

    </declare-styleable>
</resources>